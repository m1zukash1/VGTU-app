; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Uni App"
run/main_scene="res://Scenes/Main.tscn"
boot_splash/image="res://Resources/Icons/Black/icon.png"
boot_splash/bg_color=Color( 0.0784314, 0.0784314, 0.0862745, 1 )
config/icon="res://Resources/Icons/Black/icon.svg"
config/windows_native_icon="res://a9dc7-211tr-001.ico"

[autoload]

ApiContainer="*res://Singletons/ApiContainer.gd"
Cache="*res://Singletons/Cache.gd"

[display]

window/size/width=720
window/size/height=1280
window/size/always_on_top=true
window/per_pixel_transparency/allowed=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
